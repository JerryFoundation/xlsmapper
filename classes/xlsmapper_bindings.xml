<?xml version="1.0" encoding="UTF-8"?>
<!-- ============================================
 JAXBのバインディング設定
 ============================================ -->
<jxb:bindings 
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	jxb:version="2.1">
	
	<jxb:bindings schemaLocation="./xlsmapper.xsd" node="/xs:schema">
		<jxb:schemaBindings>
			<!-- <jxb:package name="proj.green.xlsmapper.xml.schema" /> -->
			<jxb:nameXmlTransform>
				<jxb:elementName suffix="Info" />
			</jxb:nameXmlTransform>
		</jxb:schemaBindings>
	</jxb:bindings>
</jxb:bindings> 